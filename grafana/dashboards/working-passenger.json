{
  "dashboard": {
    "id": null,
    "uid": "working-passenger",
    "title": "Passenger Monitoring (Working)",
    "tags": [
      "passenger"
    ],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Total Process Count",
        "type": "stat",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "$datasource"
            },
            "expr": "passenger_process_count",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Capacity Used",
        "type": "stat",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "$datasource"
            },
            "expr": "passenger_capacity_used",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 8,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Wait Queue Size",
        "type": "stat",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "$datasource"
            },
            "expr": "passenger_get_wait_list_size",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 16,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Process Memory Usage",
        "type": "timeseries",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "$datasource"
            },
            "expr": "passenger_process_memory",
            "refId": "A",
            "legendFormat": "PID {{pid}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 6
        }
      },
      {
        "id": 5,
        "title": "Process CPU Usage",
        "type": "timeseries",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "$datasource"
            },
            "expr": "passenger_process_cpu",
            "refId": "A",
            "legendFormat": "PID {{pid}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 6
        }
      }
    ],
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {},
    "refresh": "10s",
    "version": 1,
    "schemaVersion": 39,
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "Prometheus",
            "value": "Prometheus"
          },
          "hide": 0,
          "includeAll": false,
          "multi": false,
          "name": "datasource",
          "options": [],
          "query": "prometheus",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "type": "datasource"
        }
      ]
    }
  },
  "overwrite": true
}
